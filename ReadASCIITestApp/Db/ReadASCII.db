record(waveform, "$(P)ARR_COL0")
{
    field(PINI, "1")
    field(DTYP, "asynFloat64ArrayIn")
    field(INP,  "@asyn($(PORT),$(ADDR),$(TIMEOUT))ARRCOL0")
    field(FTVL, "DOUBLE")
    field(NELM, "6")
    field(LOPR, "0")
    field(HOPR, "10")
	field(SCAN, "1 second")
