record(sscan, "$(P)RAMP_TEMP") {
	field(P1PV, "$(P)IND:SP")
	field(P1SM, "LINEAR")
	field(P1SP, "0") #Start at current temp
	field(P1EP, "5") #End point of ramp
	field(P1SI, "1") #This is the amount changed per step
	#field(PDLY, "1") #The length of time for one step
	
	field(EXSC, "0")
}

record(stringin, "$(P)DIR")
{
   field(DTYP, "asynOctetRead")
   field(INP,  "@asyn($(PORT),$(ADDR),$(TIMEOUT))DIR")
   field(SCAN, "1 second")
}

record(stringout, "$(P)DIR:SP")
{
   field(DTYP, "asynOctetWrite")
   field(OUT,  "@asyn($(PORT),$(ADDR),$(TIMEOUT))DIR")
   field(OMSL, "supervisory")
}

record(ai, "$(P)ROW_NUM")
{
   field(PINI, "1")
   field(DTYP, "asynInt32")
   field(INP,  "@asyn($(PORT),$(ADDR),$(TIMEOUT))RWNU")
   field(SCAN, "I/O Intr")
}

record(ao, "$(P)ROW_NUM:SP")
{
